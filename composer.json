{
  "name": "tourze/tus-upload-server-bundle",
  "description": "TUS服务端",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
